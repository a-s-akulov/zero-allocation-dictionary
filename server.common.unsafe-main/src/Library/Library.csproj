<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <LangVersion>10</LangVersion>
        <PackageId>Server.Common.Unsafe</PackageId>
        <Version>1.0.0</Version>
        <PackageBuildNumber>0</PackageBuildNumber>
        <PackageVersion>$(Version).$(PackageBuildNumber)</PackageVersion>
        <Authors>Golenok, Akulov</Authors>
        <Company>overmindgroup</Company>
        <Product>Server.Common.Unsafe</Product>
        <Description>Общий пакет микросервисов сервера</Description>
        <RepositoryUrl>https://gitlab.overmindgroup.com/bta/backend/nuget/server.common.unsafe</RepositoryUrl>
        <PackageTags>server common unsafe golenok akulov</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.0.0" />
    </ItemGroup>

</Project>
